{
  "name": "new-admin",
  "version": "1.0.0",
  "description": "workflow",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "compile:css": "sass ./src/styles/index.scss ./src/styles/index.css --style expanded",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" ./src/styles/index.css -o ./src/styles/index.prefixed.css",
    "output:css": "node-sass ./src/styles/index.prefixed.css ./dist/assets/styles/custom.css --output-style expanded",
    "compress:css": "node-sass ./src/styles/index.prefixed.css ./dist/assets/styles/custom.min.css --output-style compressed",
    "build:css": "npm-run-all compile:css prefix:css output:css compress:css",
    "build:js": "uglifyjs ./src/scripts/index.js -o ./dist/assets/scripts/custom.js",
    "buildmin:js": "uglifyjs ./src/scripts/index.js -c -m -o ./dist/assets/scripts/custom.min.js",
    "buildjs:dev": "webpack --mode development --module-bind js=babel-loader ./src/scripts/index.js --output ./dist/assets/scripts/custom.js",
    "buildjs:prod": "webpack --mode production --module-bind js=babel-loader ./src/scripts/index.js --output ./dist/assets/scripts/custom.min.js",
    "build": "npm-run-all --parallel build:css buildjs:dev buildjs:prod"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.4.9",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-serve": "^2.0.2"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.2.1",
    "bootstrap-colorpicker": "^2.5.1",
    "bootstrap-datepicker": "^1.7.0",
    "bootstrap-daterangepicker": "^2.1.25",
    "bootstrap-slider": "^9.8.0",
    "bootstrap-timepicker": "^0.5.2",
    "font-awesome": "^4.7.0",
    "fullcalendar": "^3.4.0",
    "inputmask": "^3.3.7",
    "ion-rangeslider": "^2.2.0",
    "ionicons": "^3.0.0",
    "jquery": "^3.2.1",
    "owl.carousel": "^2.3.4",
    "requirejs": "^2.3.6",
    "select2": "^4.0.3",
    "serve": "^10.1.1",
    "slimscroll": "^0.9.1"
  }
}
